            </module>
            <module name ="TrMod1" radius="E0_L0_M1_r" dz="E_M1_pos" x1="E_M1_X1"  x2="E_M1_X2" length="E_M_length" thickness="E_M_thickness" repeat="E0_L_modulesInPhi" material="Silicon" vis="orange" sensitive="true">
